    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "deep-freeze-strict": "^1.1.1",
    "expect": "^1.20.1",
    "file-loader": "^0.8.5",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "moment": "^2.13.0",
    "node-env-file": "^0.1.8",
    "node-sass": "^3.7.0",
    "node-uuid": "^1.4.7",
