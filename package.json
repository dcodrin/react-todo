    "react-dom": "^15.0.2",
    "react-router": "^2.4.1",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
