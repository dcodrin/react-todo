{
  "name": "react-todo",
  "version": "1.0.0",
  "description": "React todo app, with foundation and mocha tests.",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test karma start",
    "dev-server": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline",
    "build": "webpack",
    "deploy": "webpack -p && git add ./public/bundle.js ./public/bundle.js.map && git commit -am 'Generated production bundle.js' && git push heroku master",
    "start": "npm run build && node server.js"
  },
  "keywords": [
    "react",
    "webpack",
    "foundation"
  ],
